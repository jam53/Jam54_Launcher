<Window x:Class="Jam54_Launcher.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Jam54_Launcher"
        mc:Ignorable="d"
        Title="Jam54 Launcher" Height="750" Width="1230">
    <Grid>
        <Grid.Background>
            <LinearGradientBrush EndPoint="0.6,1.5" StartPoint="0,0" Opacity="0.995" SpreadMethod="Pad" MappingMode="RelativeToBoundingBox">
                <GradientStop Color="Black" Offset="1"/>
                <GradientStop Color="#FF0F1330" Offset="0.36"/>
            </LinearGradientBrush>
        </Grid.Background>

    </Grid>
</Window>
